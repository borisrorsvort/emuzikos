= form_for @search, :url => users_path, :html => {:method => :get, id: "search-form-agent", class: "search-form-users"} do |f|
  .searchbar-field.hidden= f.text_field :user_type, :value => "agent", class: 'no-chosen'
  .searchbar-field.hidden= f.select :instruments_like, "", {}, :id => "search_agent_instruments_name", class: 'no-chosen'
  .searchbar-field.hidden= f.select :genres_like, "", {}, :id => "search_agent_genres_name"
  .searchbar-field= f.select :searching_for, grouped_options_for_select(searching_for_grouped_options, (params[:search][:searching_for] rescue "")), {:include_blank => t(:'search_terms.who_search_for')}, :id => "search_agent_searching_for"
  .searchbar-field= text_field_tag :location, "", :placeholder => t(:'search_terms.mass_locate')
  .searchbar-field= f.submit(t(:'buttons.search.agents'),:name => nil, :class => "btn btn-primary")




